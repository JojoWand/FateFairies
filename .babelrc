{
  "presets": ["next/babel"],
  "plugins": [
    "inline-react-svg",
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@public": "./public",
          "@icons": "./public/icons",
          "@components": "./src/components",
          "@entities": "./src/entities",
          "@providers": "./src/providers/implementations/",
          "@repositories": "./src/repositories/implementations/",
          "@useCases": "./src/useCases",
          "~": "./src"
        }
      }
    ]
  ]
}
